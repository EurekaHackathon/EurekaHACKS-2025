generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    url       = env("DATABASE_URL")
    directUrl = env("DIRECT_URL")
}

model Subscriber {
    id        Int      @id @unique @default(autoincrement())
    email     String   @unique
    createdAt DateTime @default(now())
}
